<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- Warning: tags are case-sensitive -->
<!-- This XML file is dedicated to handle custom tilesets (icon sets) in customROR -->
<!-- If you deliver a mod with empires.dat containing additional tilesets, deliver this file also. -->
<customROR_tilesets>
	<!-- tilesets: count must be >= 5. Warning: IDs go from 0 to n-1. -->
	<tilesets count="6"/>
	
	<!-- Additional tilesets -->
	<!-- Custom tileset IDs are >= 5. However it is allowed to override standard tilesets (0-4). 
		Name attribute is optional. if id>=tilesets.count, then this tileset will be ignored -->
	<tileset id="5" name="exampleLikeEgypt">
		<!-- theme_info: the 'bina' object that defines various values for background, colors, buttons... Standard=50006-50009 and 53000 -->
		<theme_info slpid="50006"/>
		<!-- slpid for checkboxes, listboxes and some other stuff. Standard=50605-50608 and 53004 -->
		<checkboxes slpid="50605"/>
		<!-- Icons for buildings. Standard=50704-50707 and 53005 -->
		<building_icons slpid="50704"/>
		<!-- Used for background in button board. Standard=50713-50717 and 53006 (low), 
			50717-50720 and 53007 (medium), 50747-50750 and 53008 (high resolution) -->
		<button_board slpid_low="50713" slpid_medium="50717" slpid_high="50747"/>
		<!-- Icons for common commands. All standard tilesets use the SLP with id=50725. -->
		<common_command_icons slpid="50725"/>
		<!-- Contains top and bottom images for game screen. Standard=50733-50736 and 53010 (low), 
			50737-50740 and 53011 (medium), 50741-50744 and 53012 (high resolution) -->
		<game slpid_low="50733" slpid_medium="50737" slpid_high="50741"/>
		<!-- button_borders: values must be between 0 and 255 -->
		<button_border_colors out_top_right="126" mid_top_right="109" in_top_right="111"
			in_bottom_left="111" mid_bottom_left="238" out_bottom_left="56"/>
	</tileset>
	
</customROR_tilesets>
